{
  "name": "@shuriken-ui/nuxt",
  "type": "module",
  "version": "3.6.1",
  "packageManager": "pnpm@9.12.3",
  "author": "Css Ninja <hello@cssninja.io> (https://cssninja.io)",
  "license": "MIT",
  "homepage": "https://github.com/shuriken-ui/nuxt",
  "repository": "shuriken-ui/nuxt",
  "bugs": "https://github.com/shuriken-ui/nuxt/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "ui",
    "framework",
    "library",
    "components",
    "tailwind",
    "tailwindcss",
    "design-system",
    "module"
  ],
  "exports": {
    ".": "./nuxt.config.ts",
    "./component-meta": {
      "types": "./.component-meta/component-meta.d.ts",
      "import": "./.component-meta/component-meta.mjs",
      "require": "./.component-meta/component-meta.cjs"
    }
  },
  "files": [
    ".component-meta",
    "app.config.ts",
    "assets",
    "components",
    "composables",
    "nuxt.config.ts",
    "nuxt.schema.ts"
  ],
  "scripts": {
    "dev": "nuxt dev .playground --open",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "coverage": "vitest run --coverage",
    "test": "run-p test:*",
    "test:unit": "vitest test",
    "test:tsc": "vue-tsc --noEmit",
    "test:lint": "run-s lint:eslint lint:prettier",
    "release": "run-s test release:*",
    "release:standard-version": "standard-version",
    "release:publish": "git push --follow-tags origin main && npm publish",
    "prepare": "simple-git-hooks && nuxt prepare .playground",
    "prepack": "nuxt-component-meta --no-schema --outputDir='../.component-meta' .playground"
  },
  "dependencies": {
    "@headlessui-float/vue": "^0.15.0",
    "@headlessui/vue": "^1.7.23",
    "@nuxt/icon": "^1.7.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@vueuse/nuxt": "^11.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@nuxt/kit": "^3.13.2",
    "@nuxt/test-utils": "^3.14.4",
    "@shuriken-ui/tailwind": "4.0.0-alpha.3",
    "@tailwindcss/vite": "4.0.0-alpha.31",
    "@types/node": "22.9.0",
    "@vitest/coverage-v8": "2.1.4",
    "@vitest/ui": "2.1.4",
    "@vue/shared": "^3.5.12",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^11.2.0",
    "commitlint": "^19.5.0",
    "eslint": "9.14.0",
    "happy-dom": "^15.11.0",
    "jiti": "^2.4.0",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.14.159",
    "nuxt-component-meta": "^0.9.0",
    "pathe": "^1.1.2",
    "playwright-core": "^1.48.2",
    "simple-git-hooks": "^2.11.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "4.0.0-alpha.31",
    "typescript": "^5.6.3",
    "unplugin-fonts": "^1.1.1",
    "vitest": "^2.1.4",
    "vue": "^3.5.12",
    "vue-tsc": "^2.1.10"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "tailwindcss",
        "postcss",
        "vite",
        "vue"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint -e -V "
  },
  "lint-staged": {
    "*.vue": [
      "eslint --fix"
    ],
    "*.{js,json}": [
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "eslint --fix"
    ]
  }
}
